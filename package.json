    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "mocha": "^6.0.2",
    "mysql2": "^1.6.5"
  },
  "dependencies": {
    "@rockyli/random": "^1.1.0",
    "@rockyli/timestamp": "^1.0.0",
    "lodash": "^4.17.11",
    "sequelize": "^5.2.3"
  }, 
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockyliyanlok/node-sequelize-builder.git"
